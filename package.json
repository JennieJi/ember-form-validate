{
  "name": "ember-form-validate",
  "version": "0.0.1-beta.11",
  "description": "Ember form validate addon",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "./node_modules/.bin/ember build",
    "start": "./node_modules/.bin/ember server",
    "test": "./node_modules/.bin/ember try:each",
    "docs": "./node_modules/.bin/jsdoc2md \"addon/**/*.js\" > DOC.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JennieJi/ember-form-validate.gits"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": {
    "name": "Jennie Ji",
    "email": "jennie.ji@hotmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "2.5.0",
    "ember-cli": "^2.10.0",
    "ember-cli-app-version": "2.0.1",
    "ember-cli-dependency-checker": "^1.3.0",
    "ember-cli-inject-live-reload": "1.4.1",
    "ember-cli-qunit": "3.1.0",
    "ember-cli-release": "1.0.0-beta.2",
    "ember-cli-sri": "2.1.1",
    "ember-cli-test-loader": "1.1.1",
    "ember-cli-uglify": "1.2.0",
    "ember-disable-prototype-extensions": "1.1.0",
    "ember-export-application-global": "1.1.1",
    "ember-load-initializers": "0.6.3",
    "ember-resolver": "2.1.1",
    "jsdoc-to-markdown": "^2.0.1",
    "loader.js": "4.1.0"
  },
  "keywords": [
    "ember-addon",
    "validator",
    "validation",
    "validate",
    "form",
    "async"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.2.1",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "versionCompatibility": {
      "ember": ">=2.2.0-beta.6 <3.0.0"
    }
  }
}
